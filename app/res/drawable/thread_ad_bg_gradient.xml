<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/PaletteSelector"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="@color/PaletteLightGray"
                      android:endColor="@color/PaletteDarkGray"
                      android:angle="270"/>
        </shape>
    </item>
</selector>
